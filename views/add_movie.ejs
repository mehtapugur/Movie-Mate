<%- include('partials/_header') -%>
  <div class="tm-page-wrap mx-auto">
    <div class="position-relative">
      <div class="tm-welcome-container tm-fixed-header tm-fixed-header-3">
        <div class="text-center">
          <p class="pt-5 px-3 tm-welcome-text tm-welcome-text-2 mb-1 mt-lg-0 mt-5 text-white mx-auto">
            Talk to Us<br>about any question you have
          </p>
        </div>
      </div>
      <div id="tm-fixed-header-bg"></div>
    </div>

    <!-- Page content -->
    <main>
      <div class="container">
        <div class="mx-auto tm-content-container">
          <div class="row mt-3pb-3">
            <div class="col-12">
              <div class="mx-auto tm-about-text-container px-3">
                <h2 class="tm-page-title mb-4 tm-text-primary">Add Datas</h2>
                <p>
                  You can upload your Datas from this area and view your pdf files on the special Documents page
                  and perform operations such as deleting, updating and querying.
                </p>
              </div>
            </div>
          </div>
          <div class="mx-auto pb-3 tm-about-text-container">
            <div class="row">
              <div class="col-lg-12 mb-5">
                <form method="post" action="/users/movies" class="tm-contact-form">
                  <div class="form-group">
                    <input type="name" name="name" id="name" class="form-control rounded-0"
                      placeholder="Data Name (not required)">
                  </div>

                  <div class="form-group">
                    <textarea rows="8" type="description" name="description" id="description"
                      class="form-control rounded-0" placeholder="Data Description (not required)"></textarea>
                  </div>
                  <!--
                  <div class="from-group">
                    <input type="file" name="pdf" class="form-control-file rounded-0">
                  </div>
                  -->
                  <div class="form-group mb-0">
                    <button type="submit" id="submit"
                      class="btn btn-primary rounded-0 d-block ml-auto mr-0 tm-btn-animate tm-btn-submit tm-icon-submit"><span>Submit</span></button>
                  </div>
                </form>
              </div>

            </div>
          </div>
        </div>

      </div>
    </main>

    <div class="container-fluid tm-content-container mx-auto pt-4">
      <%- include('partials/_footer') -%>
